<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.strkapps.MediTrack">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/meditrack_icon1"
        android:label="MediTrack"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MediTrack">
        <activity
            android:name="com.strkapps.MediTrack.MedicineTiming"
            android:exported="false" />
        <activity
            android:name="com.strkapps.MediTrack.MainActivity"
            android:exported="true"
            android:label="MediTrack"
            android:theme="@style/Theme.MediTrack.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>